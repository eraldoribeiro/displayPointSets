     "text": [
      "Input shape: \n",
      "\n",
      "X = [[0 4 4 0 0]\n",
      "     [0 0 2 2 0]]\n"
     ]
    }
   ],
   "source": [
    "print(\"Input shape: \\n\")\n",
    "print('X =', np.array2string(X, prefix='    '))"
